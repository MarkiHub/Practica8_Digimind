<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyDigimind"
        tools:targetApi="31">
        <activity
            android:name="lopez.marcos.mydigimind.ContrasenaActivity"
            android:exported="false"
            android:label="@string/title_activity_contrasena"
            android:theme="@style/Theme.MyDigimind" />
        <activity
            android:name="lopez.marcos.mydigimind.RegistroActivity"
            android:exported="false"
            android:label="@string/title_activity_registro"
            android:theme="@style/Theme.MyDigimind" />
        <activity
            android:name="lopez.marcos.mydigimind.LoginActivity"
            android:exported="true"
            android:label="@string/title_activity_login"
            android:theme="@style/Theme.MyDigimind">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name">

        </activity>
    </application>

</manifest>